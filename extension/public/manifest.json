{
  "name": "Kidsability Checkout Assistant",
  "description": "A browser extension for checking out items from L4U.",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["worker.js"],
    "persistent": false
  },
  "permissions": ["tabs", "*://*.kidsability.org/*"]
}
